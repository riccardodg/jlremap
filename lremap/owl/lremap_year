<http://www.resourcebook.eu/lremap/owl/lremap_year#2014>
      a       <http://www.resourcebook.eu/lremap/owl/lremap_year#2014> , owl:Class ;
      rdfs:subClassOf year:Year ;
      =       <http://dbpedia.org/resource/2014> .

year:
      a       owl:Ontology ;
      rdfs:comment "Simple Year Ontology" .

